<div>

  <table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
        <th scope="col">Work Team</th>
      </tr>
    </thead>
    <tbody>
      {{#each worker}}
      <tr>
        

        <th scope="row">{{this.number}}</th>
        <td>{{this.firstName}}</td>
        <td>{{this.lastName}}</td>
        <td>{{this.workTeam}}</td>

     


      </tr>
         {{/each}}

    </tbody>
  </table>


</div>